{
    "name": "evozon/translatr-bundle",
    "description": "Adds integration with multiple translation services, as OneSky, PoEditor, Crowdin, etc.",
    "keywords": ["translation", "translator", "api", "service", "onesky", "poeditor", "crowdin"],
    "homepage": "https://github.com/balazscsaba2006/EvozonTranslatrBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Balazs Csaba",
            "email": "csaba.balazs@evozon.com"
        },
        {
            "name": "Calin Bolea",
            "email": "calin.bolea@evozon.com"
        },
        {
            "name": "Calin Pristavu",
            "email": "calin.pristavu@evozon.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "onesky/api-library-php5": "^1.0",
        "symfony/console": "^3.1",
        "symfony/config": "^3.1",
        "symfony/dependency-injection": "^3.1",
        "symfony/framework-bundle": "^3.1",
        "phpunit/phpunit": "^5.6"
    },
    "suggest": {
        "onesky/api-library-php5": "Adds API client support for OneSky integration",
        "symfony/console": "To run stuff",
